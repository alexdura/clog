
AddLoc(l, c, file) :- add <: $x + $y :>, SRC(add, l, c, _, _, file).
DivLoc(l, c, file) :- div <: $x / $y :>, SRC(div, l, c, _, _, file).
PrintfLoc(l, c, file) :- call <: printf(..) :>, SRC(call, l, c, _, _, file).

OUTPUT('PrintfLoc, "PrintfLoc.csv", "csv").
OUTPUT('AddLoc, "AddLoc.csv", "csv").
OUTPUT('DivLoc, "DivLoc.csv", "csv").
