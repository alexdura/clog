Call(fun1, fun2) :- d <: .. $fun1( .. ) { .. }:>, ID($fun1, fun1),    # function definition
	   	 c <: $fun2( .. ) :>, ANCESTOR(d, c), ID($fun2, fun2). # call expression

FunCall(fun2, c) :- c <: $fun2( .. ) :>, ID($fun2, fun2).
FunDef(fun1, d) :- d <: .. $fun1( .. ) { .. } :>, ID($fun1, fun1).

OUTPUT('Call, "Call.csv", "csv").
